import model sm-ckm_no_b_mass

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define bb = b b~

generate p p > ell+ ell- bb  @0
add process p p > ell+ ell- bb j  @1
add process p p > ell+ ell- bb j j  @2
add process p p > ell+ ell- bb j j j  @3

output DYBto2LB-4Jets_MLL-50to120_HT-400to800_madgraphMLM-pythia8 -nojpeg
